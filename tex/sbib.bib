@Article{	  wg82,
  title		= {Automata in Random Environments with Application to
		  Machine Intelligence},
  author	= {Wegman, Edward J. and Gould, Jerren},
  journal	= {Pattern Analysis and Machine Intelligence, IEEE
		  Transactions on},
  year		= {1982},
  month		= {Sept. },
  volume	= {PAMI-4},
  number	= {5},
  pages		= {485-492},
  abstract	= {Computers and brains are modeled by finite and
		  probabilistic automata, respectively. Probabilistic
		  automata are known to be strictly more powerful than finite
		  automata. The observation that the environment affects
		  behavior of both computer and brain is made. Automata are
		  then modeled in an environment. Theorem 1 shows that useful
		  environmental models are those which are infinite sets. A
		  probabilistic structure is placed on the environment set.
		  Theorem 2 compares the behavior of finite (deterministic)
		  and probabilistic automata in random environments. Several
		  interpretations of Theorem 2 are discussed which offer some
		  insight into some mathematical limits of machine
		  intelligence.},
